{
  "name": "sample-nux-example",
  "repository": "https://github.com/facebook/nuclide",
  "main": "./lib/main.js",
  "version": "0.0.0",
  "description": "A package demonstrating usage of NUXes within Nuclide.",
  "atomTestRunner": "../../lib/test-runner.js",
  "nuclide": {
    "packageType": "Atom",
    "testRunner": "apm"
  },
  "consumedServices": {
    "tool-bar": {
      "versions": {
        "^0 || ^1": "consumeToolBar"
      }
    },
    "nux-register": {
      "versions": {
        ">=0.0.0": "consumeRegisterNuxService"
      }
    },
    "nux-trigger": {
      "versions": {
        ">=0.0.0": "consumeTriggerNuxService"
      }
    }
  }
}
